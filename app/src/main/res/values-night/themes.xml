<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Radio" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="android:colorBackground">@color/ActivityColor</item>
        <item name="android:textColor">@color/textToolbar</item>
        <item name="android:statusBarColor">#000000</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="textInputStyle">@color/textInputStyle</item>

    </style>

    <style name="Theme.Radio.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Radio.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Radio.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>